#include<bits/stdc++.h>
using namespace std;
#define int long long
signed main(){
    int t;cin>>t;
    while(t--){
        int a,b,m;
        cin>>a>>b>>m;
        int res=1;
        while(b>0){
            if(b%2==1)res=(res%m*a%m)%m;
            a=(a%m*a%m)%m;
            b/=2;
        }
        cout<<res<<'\n';
    }
    return 0;
}
